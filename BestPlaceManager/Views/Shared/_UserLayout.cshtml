<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link REL="SHORTCUT ICON" HREF="../../Content/Images/bp.ico">
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />

    <script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?sensor=false"></script>
    <script src="../../Scripts/MapExtend.js" type="text/javascript"></script>

   @* <script src="../../Scripts/lightbox/js/jquery-1.7.2.min.js" type="text/javascript"></script>*@

   @* <script src="../../Scripts/jquery-ui-1.8.11.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery-ui-1.8.11.min.js" type="text/javascript"></script>*@

    <script src="../../Scripts/jquery-1.5.1.min.js" type="text/javascript"></script>

    @*<script src="../../Content/jquery-ui-1.8.16wb/js/jquery-1.6.2.min.js" type="text/javascript"></script>
    <script src="../../Content/jquery-ui-1.8.16wb/js/jquery-ui-1.8.16.custom.min.js" type="text/javascript"></script>*@
    @*<link href="../../Content/jquery-ui-1.8.16wb/css/smoothness/jquery-ui-1.8.16.custom.css" rel="stylesheet" type="text/css" />*@

    @RenderSection("HeadPanel", required: false)

    @(Html.Telerik().StyleSheetRegistrar().DefaultGroup(group => group.Add("telerik.common.css").Add("telerik.sitefinity.css").Combined(true).Compress(true)))</head>
<body>
    <div class="page">
        <div id="header">

            <div id="menu">
                <div style="display:inline-block; vertical-align:top; width:585px">
                    <span><a href="/" class="t-item t-link"><b>Bestplace</b></a></span>

                    <span style="float:right; margin-top:7px; display:inline-block;">
                        @using (Html.BeginForm("All", "Search"))
                        {
                            //Html.Hidden("categoryId", (ViewBag.CategoryId != null) ? (int)ViewBag.CategoryId : 1)
                            @Html.TextBox("request", (ViewBag.Request != null) ? (string)ViewBag.Request : "", new { style = "height:18px; width:240px" })
                            @*@Html.Hidden("categoryId", new { categoryId = (ViewBag.CategoryId != null) ? ViewBag.CategoryId : 1 })
                            @Html.TextBox("request", new { request = (ViewBag.Request != null) ? ViewBag.Request : "" }, new { style = "height:18px; width:240px" })*@
                        }
                    </span>
                </div>

                <div style="width:253px; display:inline-block; float:right">
                    @Html.Partial("/Views/Shared/_LogOnPartial.cshtml")
                </div>
            </div>
        </div>

        <div id="Main">
            <div>
                <div id="MainPanel" style="min-height:360px">
                    @RenderBody()
                </div>

                <div id="RightPanel">
                    @RenderSection("RightPanel", required: false)
                </div>
            </div>

            
         </div>
         <div id="footer" style="margin-bottom:15px">
            Copyright © 2012 <a href="http://webdesign.uitlab.com/">wikiblack web design</a>
        </div>
    </div>

    @(Html.Telerik().ScriptRegistrar().jQuery(false))

    @*@(Html.Telerik().ScriptRegistrar().DefaultGroup(group => group.Combined(true).Compress(true)))*@</body>
</html>
